#include<stdio.h>
int MonthDays(int year, int month);
{
  int a;
  switch(month)
  {
    case 1:
    case 3:
    case 5:
    case 7:
    case 8:
    case 10:
    case 12:a=31;break;
    case 2:a=28;break;
    case 4:
    case 6:
    case 9:
    case 11:a=30;break;
  }
  if(month==2&&year%4==0&&year%100!=0||month==2&&year%400==0)
    a+=1;
  return a;
}
int main()
{
  int ndays,month,year;
  scanf("%d",&year);
  for(month=1;month<=12;month++)
  {
    ndays=MonthDays(year,month);
    printf("%d ",ndays);
  }
  return 0;
}
